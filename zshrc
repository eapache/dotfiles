# setup PATH
PATH="~/dotfiles/bin:$PATH"

# environment vars
export EDITOR='vim'

# git aliases
alias g="git"
alias gb="git branch"
alias gl="git pull"
alias grba="git rebase --abort"
alias grbc="git rebase --continue"

# misc aliases
alias l="ls -lah"
alias v="vim"
alias ..="cd .."
alias be="bundle exec"
alias brk="bundle exec rake"
